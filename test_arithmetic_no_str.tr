class Vector:
    def __init__(self, x, y):
        self.x = x
        self.y = y
    
    def __add__(self, other):
        print("Vector.__add__ called")
        if isinstance(other, Vector):
            return Vector(self.x + other.x, self.y + other.y)
        return Vector(self.x + other, self.y + other)
    
    def __sub__(self, other):
        print("Vector.__sub__ called")
        if isinstance(other, Vector):
            return Vector(self.x - other.x, self.y - other.y)
        return Vector(self.x - other, self.y - other)
    
    def __mul__(self, scalar):
        print("Vector.__mul__ called")
        return Vector(self.x * scalar, self.y * scalar)
    
    def __truediv__(self, scalar):
        print("Vector.__truediv__ called")
        if scalar == 0:
            return None
        return Vector(self.x / scalar, self.y / scalar)

# Test __add__
v1 = Vector(1, 2)
v2 = Vector(3, 4)
v3 = v1 + v2
print("v1 + v2 done, v3.x =", v3.x, "v3.y =", v3.y)

# Test __sub__
v4 = v3 - v1
print("v3 - v1 done, v4.x =", v4.x, "v4.y =", v4.y)

# Test __mul__
v5 = v1 * 2
print("v1 * 2 done, v5.x =", v5.x, "v5.y =", v5.y)

# Test __truediv__
v6 = v5 / 2
print("v5 / 2 done, v6.x =", v6.x, "v6.y =", v6.y)

print("All arithmetic dunder methods work!")
