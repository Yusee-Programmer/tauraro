# Math module test
import math

print("=== Math Module Test ===")

# Test basic math functions
print("Pi:", math.pi)
print("E:", math.e)
print("Square root of 16:", math.sqrt(16))
print("2^8:", math.pow(2, 8))
print("Log of 10:", math.log(10))
print("Log base 10 of 100:", math.log10(100))
print("Sin of pi/2:", math.sin(math.pi/2))
print("Cos of pi:", math.cos(math.pi))
print("Tan of pi/4:", math.tan(math.pi/4))

# Test rounding functions
print("Ceiling of 4.2:", math.ceil(4.2))
print("Floor of 4.8:", math.floor(4.8))
print("Absolute value of -5:", math.fabs(-5))

# Test trigonometric functions
print("Degrees to radians (180):", math.radians(180))
print("Radians to degrees (pi):", math.degrees(math.pi))

# Test hyperbolic functions
print("Hyperbolic sin of 1:", math.sinh(1))
print("Hyperbolic cos of 1:", math.cosh(1))

# Test factorial
print("Factorial of 5:", math.factorial(5))

# Test GCD
print("GCD of 12 and 18:", math.gcd(12, 18))

print("Math module test completed successfully!")