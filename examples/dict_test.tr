# Test basic dictionary access

# Create a simple dictionary
data = {
    "key1": "value1",
    "key2": 42,
    "nested": {"inner_key": "inner_value"}
}

# Test access
print("data['key1']:", data["key1"])
print("data['key2']:", data["key2"])
print("data['nested']:", data["nested"])
print("nested dict access:", data["nested"]["inner_key"])

# Create nested structure like analytics
analytics = {
    "total_revenue": 100.50,
    "active_users": 42,
    "revenue_by_category": {"Electronics": 500.0, "Clothing": 200.0}
}

print("\nanalytics['total_revenue']:", analytics["total_revenue"])
print("analytics['active_users']:", analytics["active_users"])
print("analytics['revenue_by_category']:", analytics["revenue_by_category"])

# Try to iterate
rev_by_cat = analytics["revenue_by_category"]
print("\nIterating over revenue_by_category:")
for cat in rev_by_cat:
    print(f"  {cat}: {rev_by_cat[cat]}")

print("Test completed!")
