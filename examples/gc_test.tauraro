# GC (Garbage Collection) module test
import gc

print("=== GC Module Test ===")

# Test garbage collection stats
print("GC stats:", gc.get_stats())

# Test collection counts
print("Collection counts:", gc.get_count())

# Test threshold
print("GC thresholds:", gc.get_threshold())

# Test enabling/disabling
print("GC enabled:", gc.isenabled())
gc.disable()
print("GC enabled after disable:", gc.isenabled())
gc.enable()
print("GC enabled after enable:", gc.isenabled())

# Test collect
collected = gc.collect()
print("Objects collected:", collected)

print("GC module test completed successfully!")