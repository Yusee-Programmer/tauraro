# Hashlib module test
import hashlib

print("=== Hashlib Module Test ===")

# Test MD5
md5_hash = hashlib.md5()
md5_hash.update(b"Hello, Tauraro!")
print("MD5:", md5_hash.hexdigest())

# Test SHA1
sha1_hash = hashlib.sha1()
sha1_hash.update(b"Hello, Tauraro!")
print("SHA1:", sha1_hash.hexdigest())

# Test SHA256
sha256_hash = hashlib.sha256()
sha256_hash.update(b"Hello, Tauraro!")
print("SHA256:", sha256_hash.hexdigest())

# Test SHA512
sha512_hash = hashlib.sha512()
sha512_hash.update(b"Hello, Tauraro!")
print("SHA512:", sha512_hash.hexdigest())

# Test hashing strings
text = "This is a test string for hashing"
md5_text = hashlib.md5(text.encode('utf-8')).hexdigest()
sha256_text = hashlib.sha256(text.encode('utf-8')).hexdigest()
print("Text MD5:", md5_text)
print("Text SHA256:", sha256_text)

print("Hashlib module test completed successfully!")