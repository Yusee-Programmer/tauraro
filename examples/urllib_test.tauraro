# Urllib module test
import urllib

print("=== Urllib Module Test ===")

# Test urllib components
print("Urllib module loaded successfully")

# Test parsing functionality
try:
    from urllib import parse
    url = "https://example.com:8080/path?query=value&other=123"
    parsed = parse.urlparse(url)
    print("Parsed URL:")
    print("  Scheme:", parsed.scheme)
    print("  Netloc:", parsed.netloc)
    print("  Path:", parsed.path)
    print("  Query:", parsed.query)
    
    # Test query parsing
    query_params = parse.parse_qs(parsed.query)
    print("Query parameters:", query_params)
    
    # Test URL building
    new_url = parse.urlunparse(('https', 'test.com', '/newpath', '', 'param=test', ''))
    print("Built URL:", new_url)
    
except ImportError as e:
    print("Urllib parse module not available:", e)

# Test request functionality
try:
    from urllib import request
    print("Urllib request module available")
    
    # Test request object
    req = request.Request("https://httpbin.org/get")
    print("Request object created:", req.full_url)
    
except ImportError as e:
    print("Urllib request module not available:", e)

# Test error handling
try:
    from urllib import error
    print("Urllib error module available")
except ImportError as e:
    print("Urllib error module not available:", e)

print("Urllib module test completed successfully!")