# Fibonacci Sequence Example
def fibonacci(n):
    if n <= 1:
        return n
    return fibonacci(n - 1) + fibonacci(n - 2)

print("Fibonacci sequence (first 10 numbers):")
for i in range(10):
    print("fib(" + str(i) + ") = " + str(fibonacci(i)))

print("\nCalculating fibonacci(15):")
result = fibonacci(15)
print("Result: " + str(result))
