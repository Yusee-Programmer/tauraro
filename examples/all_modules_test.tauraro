# Comprehensive test of all builtin modules
print("=== Comprehensive Builtin Modules Test ===")

# List of all modules to test
modules = [
    "abc", "asyncio", "base64", "collections", "copy", "csv", "datetime",
    "exceptions", "functools", "gc", "hashlib", "httptools", "httpx", "io",
    "itertools", "json", "logging", "math", "memory", "os", "pickle",
    "random", "re", "socket", "sys", "threading", "time", "unittest",
    "urllib", "websockets"
]

print("Testing " + str(len(modules)) + " builtin modules...")

# Test each module
for module_name in modules:
    try:
        # Dynamically import the module
        __import__(module_name)
        print("✓ " + module_name)
    except Exception as e:
        print("✗ " + module_name + " - Error: " + str(e))

print("\n=== Test Summary ===")
print("All modules loaded successfully!")
print("Note: This test only verifies that modules can be imported.")
print("For detailed functionality tests, run individual module test files.")