# CSV module test
import csv
import io

print("=== CSV Module Test ===")

# Test writing CSV data
output = io.StringIO()
writer = csv.writer(output)
writer.writerow(['Name', 'Age', 'City'])
writer.writerow(['Alice', 30, 'New York'])
writer.writerow(['Bob', 25, 'Los Angeles'])
writer.writerow(['Charlie', 35, 'Chicago'])

csv_data = output.getvalue()
print("CSV Data:")
print(csv_data)

# Test reading CSV data
input_data = io.StringIO(csv_data)
reader = csv.reader(input_data)
print("Reading CSV Data:")
for row in reader:
    print(row)

# Test DictReader
input_data2 = io.StringIO(csv_data)
dict_reader = csv.DictReader(input_data2)
print("\nDictReader Output:")
for row in dict_reader:
    print(row)

print("CSV module test completed successfully!")