# Datetime module test
import datetime

print("=== Datetime Module Test ===")

# Test date
today = datetime.date.today()
print("Today's date:", today)

# Test datetime
now = datetime.datetime.now()
print("Current datetime:", now)

# Test timedelta
delta = datetime.timedelta(days=7, hours=3, minutes=30)
future = now + delta
print("Future datetime:", future)

# Test formatting
formatted = now.strftime("%Y-%m-%d %H:%M:%S")
print("Formatted datetime:", formatted)

# Test parsing
parsed = datetime.datetime.strptime("2023-12-25 15:30:00", "%Y-%m-%d %H:%M:%S")
print("Parsed datetime:", parsed)

print("Datetime module test completed successfully!")