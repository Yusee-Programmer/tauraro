# Asyncio module test
import asyncio

print("=== Asyncio Module Test ===")

async def hello_world():
    print("Hello")
    await asyncio.sleep(1)
    print("World!")

async def countdown(n):
    for i in range(n, 0, -1):
        print(f"Countdown: {i}")
        await asyncio.sleep(0.5)
    print("Done!")

async def main():
    # Run tasks concurrently
    await asyncio.gather(
        hello_world(),
        countdown(3)
    )

# Run the async program
asyncio.run(main())

print("Asyncio module test completed successfully!")