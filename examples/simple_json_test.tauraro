# Simple JSON test
import json

print("=== Simple JSON Test ===")

# Test list to JSON
data = ["Tauraro", 1.0, "fast", "efficient"]
json_string = json.dumps(data)
print("JSON string:", json_string)

# Test JSON to list
parsed_data = json.loads(json_string)
print("Parsed data:", parsed_data)

# Test simple dictionary using dict() constructor
data2 = dict()
data2["name"] = "Tauraro"
data2["version"] = 1.0
json_string2 = json.dumps(data2)
print("JSON string2:", json_string2)

print("Simple JSON test completed!")