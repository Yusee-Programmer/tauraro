# Advanced Math Module Test
import math

print("=== Advanced Math Module Test ===")

# Test new functions added
print("\n--- New Functions ---")

# Test isqrt
print("isqrt(16):", math.isqrt(16))
print("isqrt(25):", math.isqrt(25))
print("isqrt(30):", math.isqrt(30))

# Test nextafter
print("nextafter(1.0, 2.0):", math.nextafter(1.0, 2.0))
print("nextafter(1.0, 0.0):", math.nextafter(1.0, 0.0))

# Test existing functions to ensure they still work
print("\n--- Existing Functions ---")

# Test hyperbolic functions
print("sinh(1.0):", math.sinh(1.0))
print("cosh(1.0):", math.cosh(1.0))
print("tanh(1.0):", math.tanh(1.0))
print("asinh(1.0):", math.asinh(1.0))
print("acosh(2.0):", math.acosh(2.0))
print("atanh(0.5):", math.atanh(0.5))

# Test floating point operations
print("fmod(10.5, 3.2):", math.fmod(10.5, 3.2))
print("remainder(10.5, 3.2):", math.remainder(10.5, 3.2))

# Test modf
fractional, integer = math.modf(3.14159)
print("modf(3.14159): fractional =", fractional, ", integer =", integer)

# Test frexp and ldexp
mantissa, exponent = math.frexp(8.0)
print("frexp(8.0): mantissa =", mantissa, ", exponent =", exponent)
print("ldexp(0.5, 4):", math.ldexp(0.5, 4))

# Test copysign
print("copysign(5.0, -3.0):", math.copysign(5.0, -3.0))
print("copysign(-5.0, 3.0):", math.copysign(-5.0, 3.0))

# Test classification functions
print("\n--- Classification Functions ---")
print("isfinite(3.14):", math.isfinite(3.14))
print("isfinite(math.inf):", math.isfinite(math.inf))
print("isinf(math.inf):", math.isinf(math.inf))
print("isinf(3.14):", math.isinf(3.14))
print("isnan(math.nan):", math.isnan(math.nan))
print("isnan(3.14):", math.isnan(3.14))

# Test isclose
print("isclose(1.0, 1.0000001):", math.isclose(1.0, 1.0000001))
print("isclose(1.0, 1.1, rel_tol=0.2):", math.isclose(1.0, 1.1, rel_tol=0.2))

# Test special functions
print("\n--- Special Functions ---")
print("gamma(5):", math.gamma(5))  # Should be 4! = 24
print("lgamma(5):", math.lgamma(5))
print("erf(1.0):", math.erf(1.0))
print("erfc(1.0):", math.erfc(1.0))

# Test constants
print("\n--- Constants ---")
print("pi:", math.pi)
print("e:", math.e)
print("tau:", math.tau)
print("inf:", math.inf)
print("nan:", math.nan)

print("Advanced Math module test completed successfully!")