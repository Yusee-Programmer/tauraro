# Algebra module within math package
from math import PI, E, square, cube

def quadratic(a, b, c, x):
    return a * square(x) + b * x + c

def exponential(base, exponent):
    # Simple exponential function
    result = 1
    for i in range(exponent):
        result = result * base
    return result

def factorial(n):
    if n <= 1:
        return 1
    result = 1
    for i in range(2, n + 1):
        result = result * i
    return result