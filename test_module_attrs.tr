#!/usr/bin/env tauraro
# Test module attribute access

print("=== Testing Module Attribute Access ===")

import testmod

print("\n[Test 1] Module metadata attributes:")
try:
    print("  testmod.__version__ = " + str(testmod.__version__))
    print("  [OK]")
except Exception as e:
    print("  [FAIL] " + str(e))

try:
    print("  testmod.__name__ = " + str(testmod.__name__))
    print("  [OK]")
except Exception as e:
    print("  [FAIL] " + str(e))

print("\n[Test 2] Private variables:")
try:
    print("  testmod._private_var = " + str(testmod._private_var))
    print("  [OK]")
except Exception as e:
    print("  [FAIL] " + str(e))

print("\n[Test 3] Public variables:")
try:
    print("  testmod.public_var = " + str(testmod.public_var))
    print("  [OK]")
except Exception as e:
    print("  [FAIL] " + str(e))

print("\n[Test 4] Classes:")
try:
    obj = testmod.TestClass()
    print("  Created instance: " + str(obj))
    print("  [OK]")
except Exception as e:
    print("  [FAIL] " + str(e))

print("\n[Test 5] Functions:")
try:
    result = testmod.test_func()
    print("  testmod.test_func() = " + result)
    print("  [OK]")
except Exception as e:
    print("  [FAIL] " + str(e))

print("\n=== All Tests Complete ===")
