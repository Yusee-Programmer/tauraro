# Test 2: Arithmetic Operations with Type Annotations

def calculate(a: int, b: int) -> int:
    sum_val: int = a + b
    diff_val: int = a - b
    prod_val: int = a * b
    quot_val: int = a / b
    mod_val: int = a % b

    result: int = sum_val + diff_val + prod_val + quot_val + mod_val
    return result

def power(base: int, exp: int) -> int:
    result: int = 1
    i: int = 0
    while i < exp:
        result = result * base
        i = i + 1
    return result

def factorial(n: int) -> int:
    if n <= 1:
        return 1
    return n * factorial(n - 1)

# Test arithmetic
calc_result: int = calculate(100, 7)
print(calc_result)

# Test power function
power_result: int = power(2, 10)
print(power_result)

# Test factorial
fact_result: int = factorial(5)
print(fact_result)
