# Test exception handling with traceback

print("Testing exception handling with traceback")

# Test 1: Basic exception with traceback
try:
    raise ValueError("This is a test ValueError")
except ValueError as e:
    print("Caught ValueError:", e)

# Test 2: Another exception type
try:
    raise RuntimeError("This is a test RuntimeError")
except RuntimeError as e:
    print("Caught RuntimeError:", e)

