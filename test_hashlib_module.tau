#!/usr/bin/env tauraro
"""Test Hashlib Module"""

import hashlib

def main():
    print("=" * 70)
    print("HASHLIB MODULE TEST")
    print("=" * 70)
    print()
    
    print("=== Test 1: MD5 Hash ===")
    text = "hello"
    hash_result = hashlib.md5(text)
    print(f"MD5('hello'): {hash_result}")
    print()
    
    print("=== Test 2: SHA256 Hash ===")
    hash_result = hashlib.sha256(text)
    print(f"SHA256('hello'): {hash_result}")
    print()
    
    print("=" * 70)
    print("HASHLIB MODULE TEST COMPLETED")
    print("=" * 70)

if __name__ == "__main__":
    main()
