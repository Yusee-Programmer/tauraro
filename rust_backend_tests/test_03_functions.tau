# Test 3: Functions with different features
def add(a, b):
    return a + b

def greet(name="World"):
    print("Hello, " + name)

def variadic(*args):
    for arg in args:
        print(arg)

def multiply(x, y=2):
    return x * y

def fibonacci(n):
    if n <= 1:
        return n
    return fibonacci(n - 1) + fibonacci(n - 2)

def test_lambda():
    square = lambda x: x * x
    print(square(5))

def test_map_filter():
    numbers = [1, 2, 3, 4, 5]
    doubled = [x * 2 for x in numbers]
    print(doubled)

def main():
    print(add(5, 3))
    greet()
    greet("Tauraro")
    print(multiply(4))
    print(multiply(4, 3))
    print(fibonacci(6))
    test_lambda()
    test_map_filter()

if __name__ == "__main__":
    main()
