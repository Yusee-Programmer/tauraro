# Tauraro DUITK Import Test
print("=== Tauraro DUITK Import Test ===")

# Test 1: Basic import
print("Test 1: Basic import...")
try:
    import duitk
    print("✓ SUCCESS: import duitk worked")
    
    # Check if key components are available
    if hasattr(duitk, 'Application'):
        print("✓ Application class is available")
    else:
        print("✗ Application class not found")
        
    if hasattr(duitk, 'Window'):
        print("✓ Window class is available")
    else:
        print("✗ Window class not found")
        
    if hasattr(duitk, 'Button'):
        print("✓ Button class is available")
    else:
        print("✗ Button class not found")
        
    # Check version info
    version = getattr(duitk, '__version__', 'Not found')
    print(f"DUITK version: {version}")
    
except Exception as e:
    print(f"✗ FAILED: import duitk failed with error: {e}")
    import traceback
    traceback.print_exc()

print("\n=== Test Complete ===")