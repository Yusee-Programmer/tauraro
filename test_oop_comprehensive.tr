# Comprehensive OOP Test with Static Typing

class Point:
    def __init__(self, x: int, y: int):
        self.x = x
        self.y = y

    def get_x(self) -> int:
        return self.x

    def get_y(self) -> int:
        return self.y

    def set_x(self, new_x: int):
        self.x = new_x

    def set_y(self, new_y: int):
        self.y = new_y

    def distance_squared(self) -> int:
        return self.x * self.x + self.y * self.y

    def translate(self, dx: int, dy: int):
        self.x = self.x + dx
        self.y = self.y + dy

class Vector2D:
    def __init__(self, x: float, y: float):
        self.x = x
        self.y = y

    def magnitude(self) -> float:
        return (self.x * self.x + self.y * self.y) ** 0.5

    def dot(self, other) -> float:
        return self.x * other.x + self.y * other.y

    def scale(self, factor: float):
        self.x = self.x * factor
        self.y = self.y * factor

class Rectangle:
    def __init__(self, width: int, height: int):
        self.width = width
        self.height = height

    def area(self) -> int:
        return self.width * self.height

    def perimeter(self) -> int:
        return 2 * (self.width + self.height)

    def is_square(self) -> bool:
        return self.width == self.height

    def scale(self, factor: int):
        self.width = self.width * factor
        self.height = self.height * factor

# Test Point class
def test_point():
    p: Point = Point(3, 4)
    print(p.get_x())
    print(p.get_y())
    print(p.distance_squared())

    p.translate(1, 1)
    print(p.get_x())
    print(p.get_y())

# Test Rectangle class
def test_rectangle():
    rect: Rectangle = Rectangle(10, 20)
    print(rect.area())
    print(rect.perimeter())
    print(rect.is_square())

    square: Rectangle = Rectangle(5, 5)
    print(square.is_square())

# Run tests
test_point()
test_rectangle()
