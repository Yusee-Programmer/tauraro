print("Starting closure test")

def my_decorator(func):
    print("my_decorator called")
    
    def wrapper():
        print("In wrapper, about to access func")
        print(f"func is: {func}")
        return func()
    
    return wrapper

@my_decorator
def greet():
    return "Hello!"

print("Calling decorated function")
result = greet()
print(f"Result: {result}")
