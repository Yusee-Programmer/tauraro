class A:
    def __init__(self, value):
        self.value = value
    
    def __sub__(self, other):
        print("A.__sub__ was called!")
        if isinstance(other, A):
            return A(self.value - other.value)
        return A(self.value - other)

a1 = A(10)
a2 = A(3)
result = a1 - a2
print("Result value:", result.value)
