}


int function_with_params(int a, int b, int c) {
    return (a + (b * c));
}


int recursive_fibonacci(int n) {
    if ((n <= 1)) {
        return n;
    }
    return (recursive_fibonacci((n - 1)) + recursive_fibonacci((n - 2)));
}


int iterative_fibonacci(int n) {
    if ((n <= 1)) {
        return n;
    }
    int a = 0;
    int b = 1;
    int i = 2;
    while ((i <= n)) {
        int temp = (a + b);
        a = b;
        b = temp;
        i = (i + 1);
    }
    return b;
}


