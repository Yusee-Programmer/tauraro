# Test 6: Math Module with Type Annotations
import math

def calculate_circle_area(radius: float) -> float:
    return math.pi * radius * radius

def calculate_circle_circumference(radius: float) -> float:
    return 2.0 * math.pi * radius

def calculate_sphere_volume(radius: float) -> float:
    return (4.0 / 3.0) * math.pi * radius * radius * radius

def pythagorean(a: float, b: float) -> float:
    return (a * a + b * b) ** 0.5

# Test circle calculations
radius: float = 5.0
area: float = calculate_circle_area(radius)
circumference: float = calculate_circle_circumference(radius)
volume: float = calculate_sphere_volume(radius)

print(area)
print(circumference)
print(volume)

# Test Pythagorean theorem
hypotenuse: float = pythagorean(3.0, 4.0)
print(hypotenuse)
