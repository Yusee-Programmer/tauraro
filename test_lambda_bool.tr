#!/usr/bin/env tauraro
# Test lambda returning boolean
f = lambda x: x > 2
print(f"f(1) = {f(1)}")
print(f"f(3) = {f(3)}")
print(f"f(1).is_truthy() would be: {f(1)} == False")
print(f"f(3).is_truthy() would be: {f(3)} == True")

# Manually test the filtering logic
numbers = [1, 2, 3, 4, 5]
print("\nManual filter test:")
for item in numbers:
    result = f(item)
    print(f"  f({item}) = {result}, truthy={result}")
