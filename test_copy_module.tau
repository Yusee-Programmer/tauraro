#!/usr/bin/env tauraro
"""Test Copy Module"""

import copy

def main():
    print("=" * 70)
    print("COPY MODULE TEST")
    print("=" * 70)
    print()
    
    print("=== Test 1: Shallow Copy ===")
    original = [1, 2, [3, 4]]
    shallow = copy.copy(original)
    print(f"Original: {original}")
    print(f"Shallow copy: {shallow}")
    print()
    
    print("=== Test 2: Deep Copy ===")
    original = [1, 2, [3, 4]]
    deep = copy.deepcopy(original)
    print(f"Original: {original}")
    print(f"Deep copy: {deep}")
    print()
    
    print("=" * 70)
    print("COPY MODULE TEST COMPLETED")
    print("=" * 70)

if __name__ == "__main__":
    main()
