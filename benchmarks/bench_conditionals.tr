# Benchmark: Conditionals - 1 Billion Iterations
# Tests conditional branching performance

def conditional_sum(n: int) -> int:
    total: int = 0
    i: int = 0
    while i < n:
        if i % 2 == 0:
            total = total + 1
        else:
            total = total + 2
        i = i + 1
    return total

result: int = conditional_sum(1000000000)
print(result)
