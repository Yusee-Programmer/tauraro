"""Debug what error messages are actually being generated"""

# Test to see what error messages are returned
print("=" * 80)
print("DEBUGGING ERROR MESSAGES")
print("=" * 80)

# Test 1: IndexError message
print("\nTest 1: List index out of bounds")
try:
    x = [1, 2, 3][10]
except Exception as e:
    print(f"Exception type: {type(e).__name__}")
    print(f"Message: {str(e)}")

# Test 2: KeyError message
print("\nTest 2: Dict key not found")
try:
    x = {'a': 1}['b']
except Exception as e:
    print(f"Exception type: {type(e).__name__}")
    print(f"Message: {str(e)}")

# Test 3: len() error
print("\nTest 3: len() on non-sequence")
try:
    x = len(42)
except Exception as e:
    print(f"Exception type: {type(e).__name__}")
    print(f"Message: {str(e)}")

# Test 4: ZeroDivisionError
print("\nTest 4: Division by zero")
try:
    x = 10 / 0
except Exception as e:
    print(f"Exception type: {type(e).__name__}")
    print(f"Message: {str(e)}")

print("\n" + "=" * 80)
