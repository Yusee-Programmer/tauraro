"""Test exception handling with traceback information"""

print("=" * 80)
print("TEST 1: Single-level IndexError with traceback")
print("=" * 80)

try:
    x = [1, 2, 3][10]
    print("This shouldn't print")
except IndexError as e:
    print(f"✅ Caught IndexError: {e}")

print("\n" + "=" * 80)
print("TEST 2: Builtin TypeError with traceback")
print("=" * 80)

try:
    result = len(42)
    print("This shouldn't print")
except TypeError as e:
    print(f"✅ Caught TypeError: {e}")

print("\n" + "=" * 80)
print("TEST 3: Arithmetic ZeroDivisionError")
print("=" * 80)

try:
    x = 10
    y = 0
    result = x / y
    print("This shouldn't print")
except ZeroDivisionError as e:
    print(f"✅ Caught ZeroDivisionError: {e}")

print("\n" + "=" * 80)
print("TEST 4: Type mismatch error")
print("=" * 80)

try:
    result = 1 + "string"
    print("This shouldn't print")
except TypeError as e:
    print(f"✅ Caught TypeError: {e}")

print("\n" + "=" * 80)
print("TEST 5: Dictionary KeyError")
print("=" * 80)

try:
    d = {"a": 1}
    result = d["missing_key"]
    print("This shouldn't print")
except KeyError as e:
    print(f"✅ Caught KeyError: {e}")

print("\n" + "=" * 80)
print("TEST 6: ValueError from invalid conversion")
print("=" * 80)

try:
    result = int("not a number")
    print("This shouldn't print")
except ValueError as e:
    print(f"✅ Caught ValueError: {e}")

print("\n" + "=" * 80)
print("ALL EXCEPTION TRACEBACK TESTS COMPLETED")
print("=" * 80)
