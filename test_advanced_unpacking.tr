# Advanced nested unpacking test case

# Test case from user request: dict.items() with nested unpacking
print("=== Real-world Example: Processing Dictionary Items ===")
config = {
    "name": "tauraro",
    "version": "0.1.0",
    "features": ["nested_unpacking", "c_transpiler"],
    "authors": ["User"]
}

print("\nUsing enumerate and nested unpacking:")
for i, (key, value) in enumerate(config.items()):
    print(f"[{i}] {key}: {value}")

# Test case: List of tuples with nested unpacking
print("\n=== Nested Data Structures ===")
users = [
    ("alice", ("engineer", "NYC")),
    ("bob", ("designer", "LA")),
    ("charlie", ("manager", "Chicago"))
]

print("\nProcessing list of user data:")
for idx, (name, (role, city)) in enumerate(users):
    print(f"{idx}. {name} ({role}) in {city}")

# Test case: Triple nested unpacking
print("\n=== Triple Nested Unpacking ===")
data = [
    (1, (2, (3, 4))),
    (5, (6, (7, 8))),
]

for a, (b, (c, d)) in data:
    print(f"a={a}, b={b}, c={c}, d={d}")

print("\nâœ“ All advanced unpacking tests completed successfully!")
