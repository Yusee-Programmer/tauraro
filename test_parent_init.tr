print("Testing parent __init__ call...")

class Base:
    def __init__(self, value):
        print("  Base.__init__ called with:", value)
        self.value = value
        print("  self.value set to:", self.value)

class Child(Base):
    def __init__(self, value, extra):
        print("  Child.__init__ called")
        Base.__init__(self, value)
        print("  After Base.__init__")
        self.extra = extra
        print("  self.extra set to:", self.extra)

print("\nCreating Child...")
obj = Child("test_value", "extra_value")
print("\nAccessing attributes...")
print("Value:", obj.value)
print("Extra:", obj.extra)
