int test_if_else(void) {
    int count = 0;
    int i = 0;
    while ((i < 10000000)) {
        if (((i % 3) == 0)) {
            count = (count + 1);
        } else {
            count = (count + 3);
        }
        i = (i + 1);
    }
    return count;
}


int test_nested_if(void) {
    int count = 0;
    int i = 0;
    while ((i < 5000000)) {
        if (((i % 2) == 0)) {
            if (((i % 4) == 0)) {
                count = (count + 1);
            } else {
                count = (count + 2);
            }
        } else {
            if (((i % 3) == 0)) {
                count = (count + 3);
            } else {
                count = (count + 4);
            }
        }
        i = (i + 1);
    }
    return count;
}


int test_while_loop(void) {
    int total = 0;
    int i = 0;
    while ((i < 10000000)) {
        total = (total + i);
        i = (i + 1);
    }
    return total;
}


int test_nested_while(void) {
    int total = 0;
    int i = 0;
    while ((i < 1000)) {
        int j = 0;
        while ((j < 1000)) {
            total = (total + 1);
            j = (j + 1);
        }
        i = (i + 1);
    }
    return total;
}


int test_break_continue(void) {
    int total = 0;
    int i = 0;
    while ((i < 10000000)) {
        if (((i % 2) == 0)) {
            i = (i + 1);
            // Unsupported statement: Continue
        }
        if ((i > 9999990)) {
            // Unsupported statement: Break
        }
        total = (total + i);
        i = (i + 1);
    }
    return total;
}


