# Test nested function calls (the original issue we fixed)
class Person:
    def __init__(self, name, age):
        self.name = name
        self.age = age
    
    def greet(self):
        return f"Hello, my name is {self.name} and I am {self.age} years old"

# Create an instance
person = Person("Alice", 30)

# Test nested function calls - this was the original issue
print(person.name)  # Should print "Alice"
print(person.age)   # Should print 30
print(person.greet())  # Should print "Hello, my name is Alice and I am 30 years old"

