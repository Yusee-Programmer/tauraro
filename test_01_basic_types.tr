# Test 1: Basic Types with Type Annotations

def add_integers(a: int, b: int) -> int:
    return a + b

def add_floats(x: float, y: float) -> float:
    return x + y

def concat_strings(s1: str, s2: str) -> str:
    return s1 + s2

def logical_and(a: bool, b: bool) -> bool:
    return a and b

# Test integer operations
result_int: int = add_integers(10, 20)
print(result_int)

# Test float operations
result_float: float = add_floats(3.14, 2.86)
print(result_float)

# Test string operations
result_str: str = concat_strings("Hello, ", "World!")
print(result_str)

# Test boolean operations
result_bool: bool = logical_and(True, False)
print(result_bool)
