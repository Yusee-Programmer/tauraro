#!/usr/bin/env tauraro
# Test: Built-in functions

def test_builtins():
    print("=== Built-in Functions ===")
    
    # abs
    print(f"abs(-5) = {abs(-5)}")
    
    # min and max
    numbers = [3, 1, 4, 1, 5, 9, 2, 6]
    print(f"min({numbers}) = {min(numbers)}")
    print(f"max({numbers}) = {max(numbers)}")
    
    # sum
    total = sum([1, 2, 3, 4, 5])
    print(f"sum([1,2,3,4,5]) = {total}")
    
    # sorted
    sorted_nums = sorted([3, 1, 4, 1, 5, 9, 2, 6])
    print(f"sorted: {sorted_nums}")
    
    # reversed
    rev = list(reversed([1, 2, 3, 4, 5]))
    print(f"reversed: {rev}")
    
    # enumerate
    items = ["a", "b", "c"]
    for i, item in enumerate(items):
        print(f"  {i}: {item}")
    
    # zip
    a = [1, 2, 3]
    b = ["x", "y", "z"]
    zipped = list(zip(a, b))
    print(f"zip result: {zipped}")
    
    # any and all
    print(f"any([False, False, True]) = {any([False, False, True])}")
    print(f"all([True, True, False]) = {all([True, True, False])}")
    
    # pow
    print(f"pow(2, 3) = {pow(2, 3)}")
    
    # round
    print(f"round(3.7) = {round(3.7)}")
    
    # isinstance checks
    val = "hello"
    print(f"isinstance('hello', str) = {isinstance(val, str)}")

test_builtins()
