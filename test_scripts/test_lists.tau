#!/usr/bin/env tauraro
# Test list operations

def test_lists():
    print("=== List Test ===")
    
    # Create and manipulate lists
    nums = [1, 2, 3]
    print("Created list")
    
    # Append
    nums.append(4)
    print("Appended 4")
    
    # Loop through list
    print("List contents:")
    for n in nums:
        print(n)
    
    print("List test completed")

test_lists()
