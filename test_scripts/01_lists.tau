#!/usr/bin/env tauraro
# Test: List operations

def test_lists():
    print("=== List Operations ===")
    
    # List creation and indexing
    lst = [1, 2, 3, 4, 5]
    print(f"Original list: {lst}")
    print(f"First element: {lst[0]}")
    print(f"Last element: {lst[4]}")
    
    # List length
    length = len(lst)
    print(f"Length: {length}")
    
    # List iteration
    print("Iterating:")
    for item in lst:
        print(f"  Item: {item}")
    
    # Slicing
    slice_result = lst[1:4]
    print(f"Slice [1:4]: {slice_result}")
    
    # Append operation
    lst2 = [10, 20]
    lst2.append(30)
    print(f"After append: {lst2}")
    
    # Pop operation
    popped = lst2.pop()
    print(f"Popped value: {popped}")
    print(f"After pop: {lst2}")
    
    # Index operation
    idx = lst.index(3)
    print(f"Index of 3: {idx}")
    
    # Reverse
    lst3 = [1, 2, 3]
    lst3.reverse()
    print(f"Reversed: {lst3}")
    
    # Count
    lst4 = [1, 2, 1, 3, 1]
    count = lst4.count(1)
    print(f"Count of 1: {count}")

test_lists()
