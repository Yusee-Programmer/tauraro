#!/usr/bin/env tauraro
# Test dictionary operations

def test_dicts():
    print("=== Dictionary Test ===")
    
    # Create a dictionary
    person = {"name": "Alice", "age": "30", "city": "NYC"}
    print("Created dictionary")
    
    # Access values
    name = person["name"]
    print("Name: ")
    print(name)
    
    age = person["age"]
    print("Age: ")
    print(age)
    
    print("Dictionary test completed")

test_dicts()
