# Test simple arithmetic without f-strings causing issues

def test_arithmetic():
    print("=== Arithmetic Test ===")
    
    # Simple variables
    x = 10
    y = 20
    z = x + y
    
    # Print results
    print("Variables assigned")
    
    # More arithmetic
    a = z - 5
    b = x * 2
    c = y // 3
    
    print("All operations completed")
    
    # Loop test
    print("Loop test:")
    for i in range(5):
        print(i)
    
    print("Done")

test_arithmetic()
