# Exception Handling Example
# Demonstrates exception handling in Tauraro

def main():
    print("=== EXCEPTION HANDLING EXAMPLES ===")
    print("")
    
    # Basic try/except
    try:
        result = 10 / 2
        print("10 / 2 =", result)
    except:
        print("Error occurred")
    
    print("")
    print("Exception handling working correctly!")

if __name__ == "__main__":
    main()
