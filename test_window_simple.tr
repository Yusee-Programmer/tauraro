import guidesktop

print("Initializing...")
guidesktop.init()

print("\nCreating window...")
window = guidesktop.Window("Test", 800, 600)
print("Window created")
print("Window title:", window.title)
print("Window widgets:", window.widgets)
print("Widget count:", len(window.widgets))

print("\nCreating button...")
button = guidesktop.Button(50, 50, 200, 60, "Click Me!", None)
print("Button created")
print("Button text:", button.text)

print("\nAdding button to window...")
window.add_widget(button)
print("Button added")
print("Widget count:", len(window.widgets))

print("\nDone!")
