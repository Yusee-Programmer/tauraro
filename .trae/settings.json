{
  "files.associations": {
    "*.tr": "tauraro",
    "*.tauraro": "tauraro"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.control.tauraro",
        "settings": {
          "foreground": "#569CD6",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "keyword.declaration.tauraro",
        "settings": {
          "foreground": "#569CD6",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "keyword.operator.logical.tauraro",
        "settings": {
          "foreground": "#569CD6"
        }
      },
      {
        "scope": "string.quoted.double.tauraro",
        "settings": {
          "foreground": "#CE9178"
        }
      },
      {
        "scope": "string.interpolated.tauraro",
        "settings": {
          "foreground": "#D7BA7D"
        }
      },
      {
        "scope": "constant.numeric.tauraro",
        "settings": {
          "foreground": "#B5CEA8"
        }
      },
      {
        "scope": "comment.line.tauraro",
        "settings": {
          "foreground": "#6A9955",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.name.function.tauraro",
        "settings": {
          "foreground": "#DCDCAA"
        }
      },
      {
        "scope": "entity.name.type.class.tauraro",
        "settings": {
          "foreground": "#4EC9B0"
        }
      },
      {
        "scope": "support.function.builtin.tauraro",
        "settings": {
          "foreground": "#DCDCAA"
        }
      },
      {
        "scope": "entity.name.function.decorator.tauraro",
        "settings": {
          "foreground": "#FFFF00"
        }
      }
    ]
  },
  "editor.semanticHighlighting.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true
}