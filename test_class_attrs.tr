# Test class attributes
class Color:
    def __init__(self, r, g, b, a):
        self.r = r
        self.g = g
        self.b = b
        self.a = a

    def __str__(self):
        return "Color(" + str(self.r) + ", " + str(self.g) + ", " + str(self.b) + ", " + str(self.a) + ")"

class Colors:
    WHITE = Color(1.0, 1.0, 1.0, 1.0)
    BLACK = Color(0.0, 0.0, 0.0, 1.0)
    RED = Color(1.0, 0.0, 0.0, 1.0)

print("Testing class attributes:")
print("Colors.WHITE =", Colors.WHITE)
print("Colors.BLACK =", Colors.BLACK)
print("Colors.RED =", Colors.RED)
