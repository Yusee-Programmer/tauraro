class Value:
    def __init__(self, num):
        self.num = num
    
    def __eq__(self, other):
        print("Value.__eq__ called")
        if isinstance(other, Value):
            return self.num == other.num
        return self.num == other
    
    def __lt__(self, other):
        print("Value.__lt__ called")
        if isinstance(other, Value):
            return self.num < other.num
        return self.num < other
    
    def __gt__(self, other):
        print("Value.__gt__ called")
        if isinstance(other, Value):
            return self.num > other.num
        return self.num > other
    
    def __le__(self, other):
        print("Value.__le__ called")
        if isinstance(other, Value):
            return self.num <= other.num
        return self.num <= other
    
    def __ge__(self, other):
        print("Value.__ge__ called")
        if isinstance(other, Value):
            return self.num >= other.num
        return self.num >= other
    
    def __ne__(self, other):
        print("Value.__ne__ called")
        if isinstance(other, Value):
            return self.num != other.num
        return self.num != other

# Test comparisons
v1 = Value(5)
v2 = Value(3)
v3 = Value(5)

print("v1 == v3:", v1 == v3)
print("v1 != v2:", v1 != v2)
print("v1 > v2:", v1 > v2)
print("v1 < v2:", v1 < v2)
print("v1 >= v2:", v1 >= v2)
print("v1 <= v2:", v1 <= v2)

print("All comparison dunder methods work!")
