# Test control flow structures
print("Testing control flow...")

# If statements
x = 10
if x > 5:
    print("x is greater than 5")
elif x == 5:
    print("x is equal to 5")
else:
    print("x is less than 5")

# For loops
print("Counting from 1 to 5:")
for i in range(1, 6):
    print(i)

# While loops
print("Counting down from 5:")
count = 5
while count > 0:
    print(count)
    count -= 1

# List iteration
fruits = ["apple", "banana", "cherry"]
print("Fruits:")
for fruit in fruits:
    print(fruit)

print("All control flow tests passed!")