# Test 3: Control Flow with Type Annotations

def fibonacci(n: int) -> int:
    if n <= 1:
        return n

    a: int = 0
    b: int = 1
    i: int = 2

    while i <= n:
        temp: int = a + b
        a = b
        b = temp
        i = i + 1

    return b

def is_prime(n: int) -> bool:
    if n <= 1:
        return False
    if n <= 3:
        return True

    if n % 2 == 0 or n % 3 == 0:
        return False

    i: int = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i = i + 6

    return True

def max_of_three(a: int, b: int, c: int) -> int:
    max_val: int = a
    if b > max_val:
        max_val = b
    if c > max_val:
        max_val = c
    return max_val

# Test Fibonacci
print(fibonacci(10))

# Test prime checking
print(is_prime(17))
print(is_prime(18))

# Test max function
print(max_of_three(45, 67, 23))
