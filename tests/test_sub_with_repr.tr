class Vector:
    def __init__(self, x, y):
        self.x = x
        self.y = y
    
    def __repr__(self):
        return f"Vector({self.x}, {self.y})"
    
    def __sub__(self, other):
        print("Vector.__sub__ was called!")
        return Vector(self.x - other.x, self.y - other.y)

v1 = Vector(10, 20)
v2 = Vector(3, 4)

print("v1 =", v1)
print("v2 =", v2)
print("type(v1) =", type(v1))
print("Attempting v1 - v2...")
v3 = v1 - v2
print("v3 =", v3)
