class A:
    def __init__(self, val):
        self.val = val
    
    def __sub__(self, other):
        print("A.__sub__ was called!")
        return A(self.val - other.val)
    
    def call_sub(self, other):
        print("Calling __sub__ directly...")
        result = self.__sub__(other)
        return result

a1 = A(10)
a2 = A(20)

print("Direct call via a1.call_sub(a2):")
a3 = a1.call_sub(a2)
print("Done!")

print("\nUsing - operator:")
a4 = a1 - a2
print("Done!")
