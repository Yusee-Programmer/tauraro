# Test simple generator yield
def simple_gen():
    yield 1
    yield 2
    yield 3

# Test generator in for loop (this likely causes the infinite loop)
gen = simple_gen()
count = 0
for x in gen:
    print(x)
    count = count + 1
    if count > 5:
        print("ERROR: Infinite loop detected!")
        break

print("Test completed")
