# Simple test for multiline import parsing
# Testing syntax only - not actual module imports

print("Testing multiline import syntax parsing...")

# This will test if the parser accepts these import statements
# Even if the modules don't exist, the syntax should parse correctly

try:
    # Style 1: Single line
    from math import sin, cos, tan
    print("✓ Single line import parsed")
except Exception as e:
    print(f"✗ Single line import failed: {e}")

try:
    # Style 2: With parentheses
    from math import (sqrt, pow, floor)
    print("✓ Parentheses import parsed")
except Exception as e:
    print(f"✗ Parentheses import failed: {e}")

print("\nAll syntax tests completed!")
