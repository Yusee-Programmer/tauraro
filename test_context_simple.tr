#!/usr/bin/env tauraro

print("Before context")

class MyContext:
    def __enter__(self):
        print("Entering context")
        return self
    
    def __exit__(self, exc_type, exc_val, exc_tb):
        print("Exiting context")
        return False

print("About to enter with block")
with MyContext() as ctx:
    print("Inside context")

print("After context")
